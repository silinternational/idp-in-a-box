[
  {
    "volumesFrom": [],
    "memory": ${memory},
    "extraHosts": null,
    "dnsServers": null,
    "disableNetworking": null,
    "dnsSearchDomains": null,
    "portMappings": [
      {
        "hostPort": 0,
        "containerPort": 80,
        "protocol": "tcp"
      }
    ],
    "hostname": null,
    "essential": true,
    "entryPoint": null,
    "mountPoints": [],
    "name": "web",
    "ulimits": null,
    "dockerSecurityOptions": null,
    "environment": [
      {
        "name": "APP_ID",
        "value": "${appconfig_app_id}"
      },
      {
        "name": "AWS_REGION",
        "value": "${aws_region}"
      },
      {
        "name": "ENV_ID",
        "value": "${appconfig_env_id}"
      },
      {
        "name": "CONFIG_ID",
        "value": "${appconfig_config_id}"
      },
      {
        "name": "ADMIN_EMAIL",
        "value": "${admin_email}"
      },
      {
        "name": "ADMIN_NAME",
        "value": "${admin_name}"
      },
      {
        "name": "ADMIN_PASS",
        "value": "${admin_pass}"
      },
      {
        "name": "BASE_URL_PATH",
        "value": "${base_url}"
      },
      {
        "name": "DELETE_REMEMBER_ME_ON_LOGOUT",
        "value": "${delete_remember_me_on_logout}"
      },
      {
